        xmlns:msg="http://www.example.org/messages"
        xmlns:s="http://www.example.org/businessSchemas"
        elementFormDefault="qualified"
        targetNamespace="http://www.example.org/messages">    

    <import namespace="http://www.example.org/businessSchemas" schemaLocation="CommitFault.xsd"/>

    <complexType name="CheckMergeFaultType">
        <sequence>
            <element name="fault" type="s:CommitFaultType"/>
        </sequence>
    </complexType>

    <element name="checkMergeFault" type="msg:CheckMergeFaultType"/>
</schema>
